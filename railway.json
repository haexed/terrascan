{
    "$schema": "https://railway.com/railway.schema.json",
    "build": {
        "builder": "NIXPACKS"
    },
    "deploy": {
        "startCommand": "python database/add_deduplication.py && python run.py",
        "healthcheckPath": "/",
        "healthcheckTimeout": 30,
        "restartPolicyType": "ON_FAILURE",
        "restartPolicyMaxRetries": 2
    },
    "watchPatterns": [
        "/**"
    ]
}
